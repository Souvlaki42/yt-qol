{
	"manifest_version": 3,
	"name": "YT timestamp saver",
	"version": "1.0",
	"permissions": ["activeTab"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["contentScript.js"]
		}
	],
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"action": {
		"default_icon": "images/icon48.png"
	}
}
